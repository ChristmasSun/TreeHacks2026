# Zoom RTMS Credentials
ZOOM_CLIENT_ID=your_client_id
ZOOM_CLIENT_SECRET=your_client_secret
ZOOM_SECRET_TOKEN=your_webhook_secret_token

# Server Config
PORT=3002
WEBHOOK_PATH=/webhook
MODE=webhook

# For WebSocket event delivery (alternative to webhooks)
# zoomWSURLForEvents=wss://ws.zoom.us/ws?subscriptionId=xxxxx

# Frontend WebSocket URL (for browser clients)
FRONTEND_WSS_URL_TO_CONNECT_TO=ws://localhost:3002/ws

# Python Backend (HeyGen integration)
PYTHON_BACKEND_URL=http://localhost:8000

# Deepgram TTS (optional - for audio responses)
DEEPGRAM_API_KEY=your_deepgram_api_key

# OpenRouter / LLM (optional - for AI responses)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_MODEL=anthropic/claude-3-haiku
